{
    "name": "andrey-helldar/lang-translations",
    "description": "Translation of main messages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "suggest": {
        "andrey-helldar/laravel-lang-publisher": "Publisher lang files for the Laravel Framework from caouecs/Laravel-lang",
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "require": {
        "php": ">=5.6.9",
        "illuminate/console": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\LangTranslations\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "1.x": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Helldar\\LangTranslations\\ServiceProvider"
            ]
        }
    }
}
