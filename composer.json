{
    "name": "andrey-helldar/lang-translations",
    "description": "Extended translation for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "languages", "lang", "translations", "messages"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "suggest": {
        "andrey-helldar/laravel-lang-publisher": "Publisher lang files of 68 languages for Laravel Framework 5.3+",
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "andrey-helldar/pretty-array": "^1.0",
        "andrey-helldar/support": "^1.5.0",
        "illuminate/console": "^5.5|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\LangTranslations\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\LangTranslations\\ServiceProvider"
            ]
        }
    }
}
